      },
      "minecraft:on_death": {
        "event": "death_event"
      }
    },
    "events": {
      "death_event": {
        "sequence": [
          {
            "run_command": {
              "command": [
                "damage @e[family=organ_totem,r=64] 3500"
              ]
            }
          }
        ]
      },
      "minecraft:entity_spawned": {
        "sequence": [
          {
            "filters": { "test": "is_difficulty", "value": "easy" },
            "add": {
              "component_groups": [
                "status_easy"
              ]
            }
          },
          {
            "filters": { "test": "is_difficulty", "value": "hard" },
            "add": {
              "component_groups": [
                "status_hard"
              ]
            }
          },
          {
            "filters": {
              "any_of": [
                {
                  "test": "has_biome_tag",
                  "value": "nether"
                }
              ]
            },
            "add": {
              "component_groups": [
                "nether"
              ]
            }
          },
          {
            "randomize": [
              {
                "weight": 1,
                "add": { "component_groups": [ "avoid" ] }
              },
              {
                "weight": 1,
                "add": { "component_groups": [ "follow" ] }
              }
            ]
          },
          {
            "add": {
              "component_groups": [
                "stun_enable",
                "wake"
              ]
            }
          }
        ]
      },
      "to_avoid": {
        "remove": { "component_groups": [ "follow" ] },
        "add": { "component_groups": [ "avoid" ] }
      },
      "to_follow": {
        "remove": { "component_groups": [ "avoid" ] },
        "add": { "component_groups": [ "follow" ] }
      },
      "stun": {
        "remove": { "component_groups": [ "avoid", "follow", "stun_enable", "wake" ] },
        "add": { "component_groups": [ "stun", "stun_disable" ] }
      },
      "wakeup": {
        "remove": { "component_groups": [ "stun" ] },
        "add": { "component_groups": [ "avoid","wake"] }
      }
    }

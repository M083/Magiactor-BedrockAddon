{
	"format_version": "1.13.0",

	"minecraft:feature_rules": {
		"description": {
			"identifier": "mabe:water_hole_feature_rule",
			"places_feature": "mabe:water_hole_feature"
		},

		"conditions": {
			"placement_pass": "after_underground_pass",

			"minecraft:biome_filter": {
				"all_of": [
					{
						"test": "has_biome_tag",
						"operator": "!=",
						"value": "nether"
					},
					{
						"test": "has_biome_tag",
						"operator": "!=",
						"value": "the_end"
					}
				]
			}
		},

		"distribution": {
			"iterations": 16,

			"x": {
                "distribution": "uniform",
                "extent": [
                    0,
                    16
                ]
			},
			"z": {
                "distribution": "uniform",
                "extent": [
                    0,
                    16
                ]
			},
			"y": {
				"distribution": "uniform",
				"extent": [
					"(q.noise(v.worldx / 1024, v.worldz / 1024) + q.noise(v.worldx, v.worldz) * 0.05 > 0.35 && (math.abs(v.worldx) > 200 || math.abs(v.worldz) > 200)) ? -64 : -9999",
					"(q.noise(v.worldx / 1024, v.worldz / 1024) + q.noise(v.worldx, v.worldz) * 0.05 > 0.35 && (math.abs(v.worldx) > 200 || math.abs(v.worldz) > 200)) ? 30 : -9999"
				]
			}
		}
	}
}
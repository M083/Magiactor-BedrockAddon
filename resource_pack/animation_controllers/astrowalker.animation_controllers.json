{
	"format_version" : "1.8.0",
	"animation_controllers" : {
		"controller.animation.astrowalker.walk" : {
			"states" : {
				"default": {
					"parameters": [ "query.modified_move_speed * 0.2" ],
					"animations": [
						{ 
							"walk": [
								{
									"0.0": 0.0,
									"1.0": 1.0
								}
							]
						}
					]
				}
			}
		},
		"controller.animation.astrowalker.stay" : {
			"states" : {
				"default": {
					"animations": [
						{ 
							"stay": []
						}
					]
				}
			}
		},
	    "controller.animation.astrowalker.attack": {
	      "states": {
	        "default": {
	          "transitions": [
	            {
	              "attack": "variable.attack_time > 0.0"
	            }
	          ]
	        },
	        "attack": {
	          "animations": [ { "attack": [] } ],
	          "transitions": [
	            {
	              "default": "variable.attack_time <= 0.0"
	            }
	          ]
	        }
	      }
	    }
	}
}

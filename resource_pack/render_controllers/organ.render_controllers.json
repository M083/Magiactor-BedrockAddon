{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.organ": {
      "arrays": {
        "materials": {
          "Array.materials": [ "Material.default", "Material.invisible" ]
        },
        "textures": {
          "Array.textures": [
            "Texture.default",
            "Texture.white",
            "Texture.green",
            "Texture.yellow",
            "Texture.purple",
            "Texture.red"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [ { "*": "Array.materials[query.is_invisible]" } ],
      "textures": [ "Array.textures[query.mark_variant]" ]
    }
  }
}

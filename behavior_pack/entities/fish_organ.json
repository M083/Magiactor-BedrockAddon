                    }
                  ]
                }
              },
              "deals_damage": false
            },
            {
              "on_damage": {
                "filters": {
                  "any_of": [
                    {"test": "has_damage", "value": "projectile"},
                    {"test": "has_damage", "value": "entity_explosion"},
                    {"test": "has_damage", "value": "block_explosion"}
                  ]
                },
                "event": "stun"
              },
              "on_damage_sound_event": "break"
            }
          ]
        }
      },
      "stun_disable": {
        "minecraft:damage_sensor": {
          "triggers": [
            {
              "on_damage": {
                "filters": {
                  "all_of": [
                    {
                      "test": "is_family",
                      "subject": "other",
                      "operator": "==",
                      "value": "organ"
                    }
                  ]
                }
              },
              "deals_damage": false
            }
          ]
        }
      },
      "stun": {
        "minecraft:variant": {
          "value": 1
        },
        "minecraft:physics": {},
